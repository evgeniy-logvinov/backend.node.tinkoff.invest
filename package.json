{
  "name": "backend.node.tinkoff.invest",
  "version": "1.0.0",
  "description": "Node js tinkoff bot",
  "engines": {
    "node": ">=12.18.3"
  },
  "main": "index.js",
  "dependencies": {
    "@tinkoff/invest-openapi-js-sdk": "^1.2.16",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.17",
    "moment": "^2.29.1",
    "nodemailer": "^6.4.11",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@types/busboy": "^0.2.3",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.10.1",
    "@types/nodemailer": "^6.4.0",
    "@types/rimraf": "^3.0.0",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "aws-sdk": "2.712.0",
    "eslint": "^7.8.1",
    "eslint-plugin-notice": "^0.9.10",
    "faker": "^5.1.0",
    "form-data": "^3.0.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2",
    "ajv": "^6.12.4",
    "busboy": "^0.3.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.3",
    "ws": "^7.3.1"
  },
  "scripts": {
    "eslint": "[ \"$CI\" = true ] && eslint --quiet -f codeframe --ext js,ts . || eslint --ext js,ts .",
    "eslint:fix": "[ \"$CI\" = true ] && eslint --quiet -f codeframe --ext js,ts . --fix || eslint --ext js,ts . --fix",
    "lint": "npm run eslint && npm run tsc",
    "tsc": "tsc -p .",
    "clean": "rm -f tsconfig.tsbuildinfo && docker-compose -f ./docker-compose-dev.yml rm --force && rimraf app",
    "test": "npm run lint && npm run test:api",
    "test:api": "jest --testPathPattern=test/",
    "build": "tsc -p .",
    "prestart": "npm run build",
    "start": "node .",
    "start:inspect": "npm run build && node --inspect=0.0.0.0:9229 .",
    "start:dev": "npm run build && docker-compose -f docker-compose-dev.yml up --force-recreate --build --exit-code-from carrier",
    "start:in-docker:dev": "node utils/wait-for.js postgres 5432 50000 && knex migrate:latest && nodemon -L",
    "start:prod": "NODE_ENV=production nodemon index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgeniy-logvinov/backend.node.tinkoff.invest.git"
  },
  "author": {
    "name": "evgeniy.logvinov.k"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/evgeniy-logvinov/backend.node.tinkoff.invest/issues"
  },
  "homepage": "https://github.com/evgeniy-logvinov/backend.node.tinkoff.invest#readme"
}
